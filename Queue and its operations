#include<iostream>
#include<queue>
using namespace std;
int main()
{
    queue<string> q;
    q.push("vivek");
    q.push("kumar");
    q.push("purbey");
    cout<<"size of queue before pop -> "<<q.size()<<endl;
    cout<<"first element -> "<<q.front()<<endl;
    q.pop();
    cout<<"first element -> "<<q.front()<<endl;
    cout<<"size of queue after pop -> "<<q.size()<<endl;
    cout<<"empty or not -> "<<q.empty()<<endl;
    return 0;
}
