#include<iostream>
#include<stack>
using namespace std;
int main()
{
    stack<string> s; // last in first out
    s.push("vivek");
    s.push("kumar");
    s.push("purbey");
    cout<<"top element -> "<<s.top()<<endl;
    s.pop();
    cout<<"top element -> "<<s.top()<<endl;
    cout<<"size of stack -> "<<s.size()<<endl;
    cout<<"empty or not -> "<<s.empty()<<endl; // 0-false and 1-true
    return 0;
}
