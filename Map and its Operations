#include<iostream>
#include<map>
using namespace std;
int main()
{
    map<int, string> m;
    m[1]="vivek";
    m[2]="kumar";
    m[13]="purbey";
    for(auto i:m)
    {
        cout<<i.first<<endl;
    }
    m.insert({5, "bheem"});
    for(auto i:m)
    {
        cout<<i.first<<" "<<i.second<<endl;
    }
    cout<<"finding-13->"<<m.count(13)<<endl;
    return 0;
}
